{
  "$schema": "../node_modules/@lobehub/chat-plugin-sdk/schema.json",
  "version": "1",
  "identifier": "lobchat-searxng-search-plugin",
  "author": "stone-lyl",
  "homepage": "https://github.com/stone-lyl/lobechat-AI-web-search",
  "gateway": "http://localhost:3400/api/gateway",
  "meta": {
    "avatar": "https://avatars.githubusercontent.com/u/80454229?s=28&v=28",
    "tags": ["web", "search"],
    "title": "SearxNG Search Plugin",
    "description": "A smart web search that reads and analyzes pages to provide thorough answers based on SearxNG results."
  },
  "settings": {
    "type": "object",
    "required": ["BASE_URL"],
    "properties": {
      "BASE_URL": {
        "title": "Base URL",
        "type": "string",
        "format": "uri"
      }
    }
  },
  "api": [
    {
      "name": "searchWeb",
      "url": "http://localhost:3400/api/search",
      "description": "Search SearxNG for relevant web pages.",
      "parameters": {
        "required": ["query"],
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "description": "Identify what the user is seeking"
          }
        }
      }
    }
  ]
}
